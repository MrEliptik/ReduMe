{
    "name": "ReduMe",
    "version": "0.1",
    "description": "Reduce your link size in two clicks.",
    "permissions": ["activeTab", "declarativeContent", "clipboardRead", "clipboardWrite"],
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/redume16.png",
            "32": "images/redume32.png",
            "48": "images/redume64.png",
            "128": "images/redume128.png"
        }
    },
    "web_accessible_resources": ["images/redume16.png"],
    "icons": {
        "16": "images/redume16.png",
        "32": "images/redume32.png",
        "48": "images/redume64.png",
        "128": "images/redume128.png"
    },
    "manifest_version": 2
}